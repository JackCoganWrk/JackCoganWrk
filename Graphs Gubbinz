import java.util.Arrays;
import java.util.Scanner;
public class Graphs {

    //Creates graph
    String[][] array1 = new String[10][10];

    Scanner s = new Scanner(System.in);

    //Adds node A to graph

    public void NewNode() {

        for (int i = 0; i < 10; i++) {
            System.out.println("Enter node name");
            String nodeName = s.nextLine();
            array1[0][i] = nodeName;
        }
    }

    public String NodeList(){
        String arrayOut = System.out.println(Arrays.deepToString(array1));
        return arrayOut;
    }
}


